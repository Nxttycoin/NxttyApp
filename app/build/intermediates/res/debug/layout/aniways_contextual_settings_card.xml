<?xml version="1.0" encoding="utf-8"?>
<com.aniways.ui.views.ContextualSettingsView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="@dimen/aniways_contextual_card_setting_total_width"
    android:layout_marginRight="@dimen/aniways_contextual_card_setting_overflow_width"
    android:layout_marginEnd="@dimen/aniways_contextual_card_setting_overflow_width">
    <LinearLayout
        android:id="@+id/aniways_contextual_settings_card_holder"
        android:layout_width="match_parent"
        android:layout_height="@dimen/aniways_contextual_card_min_height">
        <android.support.v7.widget.CardView
            android:id="@+id/aniways_contextual_settings_card_view"
            android:layout_height="match_parent"
            android:layout_width="@dimen/aniways_contextual_card_setting_peek_width"
            android:layout_marginRight="@dimen/aniways_contextual_card_right_margin_width"
            android:layout_marginEnd="@dimen/aniways_contextual_card_right_margin_width"
            app:cardUseCompatPadding="false">
        <LinearLayout
            android:id="@+id/aniways_contextual_settings_view"
            android:layout_height="match_parent"
            android:layout_width="match_parent">
            <FrameLayout
                android:layout_width="@dimen/aniways_contextual_card_icon_holder_width"
                android:layout_height="@dimen/aniways_contextual_card_min_height"
                android:layout_gravity="center"
                android:gravity="center">
                <ImageView
                    android:id="@+id/aniways_contextual_settings_card_icon"
                    android:background="@drawable/aniways_contextual_settings"
                    android:layout_width="@dimen/aniways_contextual_card_icon_size"
                    android:layout_height="@dimen/aniways_contextual_card_icon_size"
                    android:layout_gravity="center"
                    android:contentDescription="@string/aniways_contextual_card_title_settings"/>
            </FrameLayout>
            <TextView
                android:id="@+id/aniways_contextual_settings_card_title"
                android:text="@string/aniways_contextual_card_title_settings"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textAppearance="@style/Aniways.ContextualPopup.Card.Text"
                android:layout_marginLeft="3dp"
                android:layout_marginStart="3dp"/>
        </LinearLayout>
    </android.support.v7.widget.CardView>
    </LinearLayout>
